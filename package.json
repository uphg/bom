    "build": "tsc && vite build",
    "serve": "vite preview"
  },
  "devDependencies": {
    "@types/node": "^17.0.10",
    "cz-conventional-changelog": "^3.3.0",
    "typescript": "^4.3.2",
    "vite": "^2.6.4"
